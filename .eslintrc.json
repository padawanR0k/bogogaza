{
  "extends": ["next", "prettier", "next/core-web-vitals"],
  "rules": {
    "react/jsx-curly-brace-presence": [
      "error"
    ],
    "import/order": [
      "error",
      {
        "groups": [["builtin", "external"], ["parent", "sibling", "index"]],
        "newlines-between": "always"
      }
    ]
  }
}
